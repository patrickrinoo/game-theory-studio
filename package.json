{
  "name": "game-theory-studio",
  "version": "1.0.0",
  "private": true,
  "description": "An interactive web application for Monte Carlo simulations in game theory with real-time visualizations and strategic analysis",
  "author": "Patrick Rino",
  "homepage": "https://github.com/PatrickRino/game-theory-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatrickRino/game-theory-studio.git"
  },
  "keywords": [
    "game-theory",
    "monte-carlo",
    "simulation",
    "visualization",
    "strategy",
    "nash-equilibrium"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run __tests__/integration.test.ts",
    "test:performance": "vitest run __tests__/performance.test.ts",
    "test:e2e": "vitest run __tests__/e2e-simulation.test.ts",
    "test:unit": "vitest run __tests__/**/*.test.ts --exclude __tests__/integration.test.ts --exclude __tests__/performance.test.ts --exclude __tests__/e2e-simulation.test.ts"
  },
